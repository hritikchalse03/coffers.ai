{
  "name": "coffers-ai",
  "version": "1.0.0",
  "description": "coffers.ai - A comprehensive financial research platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "vercel-build": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "financial",
    "earnings",
    "transcripts",
    "real-time",
    "investing",
    "stocks",
    "api"
  ],
  "author": "coffers.ai Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/coffers-ai.git"
  },
  "homepage": "https://coffers-ai.vercel.app"
}